<p>
	<h2>Log in</h2>
	<%= form_for(:session, url: login_path) do |f| %>
		<%= f.label :email %>
		<p>
			<%= f.email_field :email %>
		</p>
		<%= f.label :password %>
		<p>
			<%= f.password_field :password %>
		</p>
		<%= f.label :remember_me %>
		<p>
			<%= f.check_box :remember_me %>
			<!-- <span>Remember me on this computer</span> -->
		</p>
		<p>
			<%= f.submit "Login" %>
		</p>
	<% end %>
	<p>
		New User? <%= link_to "Sign up now", signup_path %>
	</p>
</p>